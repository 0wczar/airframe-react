{
    "parser": "@babel/eslint-parser",
    "plugins": [
        "react"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        },
        "babelOptions": {
            "presets": [
                "@babel/preset-react"
            ]
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "mocha": true
    },
    "extends": [
        "standard",
        //"eslint:recommended", 
        "plugin:react/recommended"
    ],
    "rules": {}
}